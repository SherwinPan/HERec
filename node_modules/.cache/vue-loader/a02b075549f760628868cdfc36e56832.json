rsonForm[i])\r\n      }\r\n      this.$axios({\r\n            method: 'post',\r\n            url: '/createPerson',\r\n            data: fd,\r\n            header: {\r\n              'Content-Type': 'multipart/form-data'  //如果写成contentType会报错\r\n            }\r\n          }\r\n      ).then(res => {\r\n        if (res.data.result === false) {\r\n          this.$message.error('注册失败，请重新处理信息');\r\n          //修改失败，重新注册\r\n        } else {\r\n          this.$message({\r\n            message: '初始化成功',\r\n            type: 'warning'\r\n          });\r\n        }\r\n      })\r\n\r\n    },\r\n\r\n  }\r\n\r\n\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}