rsonDetail(p.personId)\">{{p.personName}}</p>\r\n          </template>\r\n          <div>出生日期：{{p.personBirth}}</div>\r\n          <div>性别：{{p.personGender}}</div>\r\n          <div>出生地：{{p.personPlace}}</div>\r\n          <div>影人简介：{{p.personBio}}</div>\r\n        </el-collapse-item>\r\n\r\n      </el-collapse>\r\n\r\n\r\n      <el-pagination\r\n          background\r\n          :current-page.sync=\"curPage\"\r\n          layout=\"prev, pager, next\"\r\n          :total=\"this.personNum\"\r\n          @current-change=\"searchPerson\"\r\n      >\r\n      </el-pagination>\r\n    </el-col>\r\n\r\n  </el-row>\r\n\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"searchPerson\",\r\n  props:{\r\n    pName: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n  },\r\n  data(){\r\n    return{\r\n      personNum:0,\r\n      personList:[],\r\n      curPage: 1,\r\n      activeName:'1',\r\n    };\r\n  },\r\n  methods:{\r\n    searchPerson(){\r\n      this.$axios({\r\n        method:\"get\",\r\n        url:'/searchPerson',\r\n        params:{\r\n          pName:this.pName,\r\n          curPage:this.curPage\r\n        }\r\n      }).then(res=>{\r\n        if(res.data.result){\r\n          this.personNum=res.data.personNum;\r\n          this.personList=res.data.personList;\r\n        }else{\r\n          this.personNum=0;\r\n        }\r\n      })\r\n    },\r\n    jumpPersonDetail(pid){\r\n      // console.log(\"jump\")\r\n      this.$router.push({name:'personDetail',params:{pid:parseInt(pid)}});\r\n    },\r\n  },\r\n  created() {\r\n    this.searchPerson()\r\n  },\r\n  watch:{\r\n    pName:function (){\r\n      location.reload()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/pages"}]}